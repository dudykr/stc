{
  "processId": 0,
  "clientInfo": {
    "name": "test-harness",
    "version": "1.0.0"
  },
  "rootUri": null,
  "initializationOptions": {
    "enable": true,
    "cache": null,
    "certificateStores": null,
    "codeLens": {
      "implementations": true,
      "references": true,
      "test": true
    },
    "config": null,
    "importMap": null,
    "lint": true,
    "suggest": {
      "autoImports": true,
      "completeFunctionCalls": false,
      "names": true,
      "paths": true,
      "imports": {
        "hosts": {}
      }
    },
    "testing": {
      "args": ["--allow-all"],
      "enable": true
    },
    "tlsCertificate": null,
    "unsafelyIgnoreCertificateErrors": null,
    "unstable": false
  },
  "capabilities": {
    "textDocument": {
      "codeAction": {
        "codeActionLiteralSupport": {
          "codeActionKind": {
            "valueSet": ["quickfix", "refactor"]
          }
        },
        "isPreferredSupport": true,
        "dataSupport": true,
        "disabledSupport": true,
        "resolveSupport": {
          "properties": ["edit"]
        }
      },
      "completion": {
        "completionItem": {
          "snippetSupport": true
        }
      },
      "foldingRange": {
        "lineFoldingOnly": true
      },
      "synchronization": {
        "dynamicRegistration": true,
        "willSave": true,
        "willSaveWaitUntil": true,
        "didSave": true
      }
    },
    "workspace": {
      "configuration": true,
      "workspaceFolders": true
    },
    "experimental": {
      "testingApi": true
    }
  }
}
