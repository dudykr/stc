{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.bk": true,
    "**/node_modules": true,
    "**/target": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  // Important
  "editor.formatOnSave": true,
  "git.ignoreLimitWarning": true,
  "rust-analyzer.assist.importPrefix": "crate",
  "git.detectSubmodules": false,
  "rust-analyzer.diagnostics.disabled": [
    "unresolved-import",
    "unresolved-extern-crate",
    "missing-ok-in-tail-expr",
    "mismatched-arg-count",
    "macro-error",
    "incorrect-ident-case",
    "no-such-field",
    "missing-structure-fields"
  ],
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.updates.channel": "stable",
  "rust-analyzer.diagnostics.enable": false,
  "rust-analyzer.diagnostics.enableExperimental": false,
  "rust-analyzer.lens.methodReferences": true,
  "rust-analyzer.assist.importEnforceGranularity": true,
  // Touch bar
  "keyboard.touchbar.enabled": true,
  "keyboard.touchbar.ignored": [
    "workbench.action.navigateBack",
    "workbench.action.navigateForward",
    "workbench.action.debug.start",
    "workbench.action.debug.run"
  ],
  "touch-bar-commands.enable.cmd3": true,
  "touch-bar-commands.command.1": "typescript/type_checker/scripts/check.sh",
  "touch-bar-commands.command.2": "typescript/type_checker/scripts/test.sh",
  "touch-bar-commands.command.3": "typescript/file_analyzer/scripts/base/update.sh",
}