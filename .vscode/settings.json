{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.bk": true,
    "**/node_modules": true,
    "**/target": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  // Important
  "editor.formatOnSave": true,
  "git.ignoreLimitWarning": true,
  "rust-analyzer.assist.importPrefix": "by_self",
  "git.detectSubmodules": false,
  "rust-analyzer.diagnostics.disabled": [
    "unresolved-import",
    "unresolved-extern-crate",
    "missing-ok-in-tail-expr",
    "mismatched-arg-count",
    "macro-error",
    "incorrect-ident-case",
    "no-such-field",
    "missing-structure-fields"
  ],
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.updates.channel": "stable",
  "rust-analyzer.diagnostics.enable": false,
  "rust-analyzer.diagnostics.enableExperimental": false,
  "rust-analyzer.lens.methodReferences": true,
  "favorites.resources": [
    "typescript/errors/src/lib.rs",
    "typescript/file_analyzer/tests"
  ]
}
