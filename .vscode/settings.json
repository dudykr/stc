{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.bk": true,
    "**/node_modules": true,
    "**/target": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  // Important
  "editor.formatOnSave": true,
  "git.ignoreLimitWarning": true,
  "git.detectSubmodules": false,
  "rust-analyzer.diagnostics.enable": false,
  "rust-analyzer.procMacro.enable": true,
  "favorites.resources": [
    "typescript/errors/src/lib.rs",
    "typescript/file_analyzer/tests",
    "typescript/type_checker/tests/tsc-stats.rust-debug",
    "typescript/type_checker/tests/wip-stats.rust-debug"
  ],
  "rust-analyzer.lruCapacity": 1024,
  "rust-analyzer.trace.server": "messages",
  "rust-analyzer.server.extraEnv": {
    "RA_LOG": "info"
  }
}
